{
    "family": "docker-react-family",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
        "name": "frontend",
        "image": "rightattitudematters/docker-react-1:latest",
        "portMappings": [
            {
                "containerPort": 80,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
            "awslogs-group": "/ecs/docker-react-family",
            "awslogs-region": "ap-southeast-2",
            "awslogs-stream-prefix": "ecs"
            }
        }
        }
    ]
}
